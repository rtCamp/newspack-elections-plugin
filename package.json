{
  "name": "govpack-blocks",
  "version": "0.0.0",
  "description": "Govpack Blocks",
  "scripts": {
    "build": "wp-scripts build",
    "check-engines": "wp-scripts check-engines",
    "check-licenses": "wp-scripts check-licenses",
    "format": "wp-scripts format",
    "lint:css": "wp-scripts lint-style",
    "lint:js": "wp-scripts lint-js",
    "lint:md:docs": "wp-scripts lint-md-docs",
    "lint:pkg-json": "wp-scripts lint-pkg-json",
    "packages-update": "wp-scripts packages-update",
    "plugin-zip": "wp-scripts plugin-zip",
    "start": "wp-scripts start",
    "test:e2e": "wp-scripts test-e2e",
    "test:unit": "wp-scripts test-unit-js",
    "lint:cpd": "jscpd ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/govpack-wp/govpack-plugin.git"
  },
  "license": "GPL-2.0-or-later",
  "bugs": {
    "url": "https://github.com/govpack-wp/govpack-plugin/issues"
  },
  "dependencies": {
    "@emotion/styled": "^11.13.5",
    "@mona-health/react-input-mask": "^3.0.3",
    "@wordpress/icons": "^10.9.0",
    "axios": "^0.24.0"
  },
  "devDependencies": {
    "@wordpress/scripts": "^30.1.0",
    "jscpd": "^3.5.10"
  },
  "config": {
    "entrypoints": {
      "editor": "./src/editor/index.js",
      "admin": "./src/admin/index.js",
      "importer": "./src/importer/index.js",
      "frontend": "./src/frontend/index.js",
      "exporter": "./src/admin/exporter/index.js"
    }
  },
  "jscpd": {
    "reporters": [
      "console"
    ],
    "ignore": [
      "**/.github/**",
      "**/deprecated/**",
      "**/block.json"
    ],
    "absolute": true,
    "gitignore": true
  }
}

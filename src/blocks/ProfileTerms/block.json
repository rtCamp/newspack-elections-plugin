{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "govpack/profile-terms",
	"title": "Profile Terms",
    "editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./index.css",
	"category": "govpack",
	"supports" : {
		"anchor": true,
		"align": true,
		"align-wide": true,
		"aria-label": true,
		"color" : {
			"background" : true,
			"button": true,
			"enableContrastChecker": true,
			"gradients": true,
			"heading": true,
			"link": true,
			"text": true	
		},
		"layout" : true,
		"shadow": true, 
		"spacing": {
			"margin": true,  
			"padding": true, 
			"blockGap": true
		},
		"typography": {
			"fontSize": true,
			"lineHeight": true,
			"textAlign": true
		}
	},
	"attributes": {
		"taxonomy": {
			"type": "string"
		}
	},
	"ancestor": [ "govpack/profile-v2" ],
	"usesContext": [
		"govpack/profileId", 
		"postType",
		"govpack/fieldKey"
	]
}

{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "govpack/profile-v2",
    "editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./index.css",
	"category": "govpack",
	"supports" : {
		"anchor": true,
		"align": true,
		"align-wide": true,
		"aria-label": true,
		"color" : {
			"background" : true,
			"button": true,
			"enableContrastChecker": true,
			"gradients": true,
			"heading": true,
			"link": true,
			"text": true	
		},
		"layout" : {
			"allowSwitching": false,
			"allowVerticalAlignment": true,
			"default" : {
				"type" : "flex"
			}
		},
		"shadow": true, 
		"spacing": {
			"margin": true,  
			"padding": true, 
			"blockGap": true
		},
		"typography": {
			"fontSize": true,
			"lineHeight": true,
			"textAlign": true
		},
		"__experimentalBorder": {
			"color" : true,
			"radius" : true,
			"width" : true,
			"style" : true
		}
	},
	"attributes": {
		"queryId": {
			"type": "number"
		},
		"profileId": {
			"type": "number",
			"default": 0
		},
		"postType": {
			"type": "string",
			"default": "govpack_profiles"
		},
		"width": {
			"type": "string"
		}
	},
	"allowedBlocks": [ 
		"core/post-title", "core/post-excerpt", "core/spacer", "core/post-featured-image", "core/group", "core/paragraph", "core/image", "core/heading", "govpack/profile-terms", "govpack/profile-row" , "govpack/profile-row-group" ],
	"providesContext": {
		"govpack/profileId" : "profileId",
		"postId" : "profileId",
		"postType" : "postType",
		"queryId": "queryId"
	}
}
